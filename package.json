{
  "name": "react-spatial",
  "version": "0.0.10",
  "description": "Components to build React map apps.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "buble": "^0.19.6",
    "canvas": "^2.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-canvas-mock": "^1.1.0",
    "ol": "^5.3.0",
    "prettier": "^1.16.1",
    "proj4": "^2.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-i18next": "^10.0.4",
    "react-styleguidist": "^8.0.6",
    "react-test-renderer": "^16.7.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.0",
    "webpack": "^4.28.4"
  },
  "peerDependency": {
    "ol": "^5.3.0"
  },
  "dependencies": {
    "@atlaskit/tree": "^4.1.10",
    "node-sass": "^4.11.0",
    "react-icons": "^3.2.2",
    "resize-observer-polyfill": "^1.5.1",
    "shortid": "^2.2.14"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(ol)/)"
    ],
    "moduleNameMapper": {
      "^.+\\.scss$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "prebuild": "rm -rf build/",
    "build": "buble --no modules -i src -o build --objectAssign Object.assign --sourcemap && cp -t build package.json README.md LICENSE",
    "postbuild": "cp package.json build/ && cd src && find .  -name '*.scss' | cpio -pdm ../build",
    "format": "prettier --write 'src/**/*.js' && stylelint 'src/**/*.css' 'src/**/*.scss' --fix",
    "lint": "eslint 'src/**/*.js' && stylelint 'src/**/*.css' 'src/**/*.scss'",
    "test": "jest src",
    "test:watch": "jest src --watch",
    "coverage": "yarn test --coverage --coverageDirectory=coverage",
    "doc": "styleguidist build",
    "doc:server": "styleguidist server",
    "symlink": "yarn build && cd build && yarn link"
  }
}
